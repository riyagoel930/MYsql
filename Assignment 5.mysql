SELECT * FROM STUDENT;
SELECT ROLL,CONCAT(CONCAT(CONCAT(F_NAME,' '),CONCAT(M_NAME,' ')),L_NAME) AS "NAME" FROM  STUDENT;
SELECT ROLL,CONCAT(CONCAT(CONCAT(F_NAME,' '),CONCAT(M_NAME,' ')),L_NAME) AS "NAME",CONCAT(CONCAT(CONCAT(HOUSENO,','),CONCAT(AREA,' ')),CITY) AS ADDRESS FROM  STUDENT;
SELECT ROLL,SUBSTR(F_NAME,1,1)||'.'||SUBSTR(M_NAME,1,1)||'.'|| L_NAME AS "NAME" FROM STUDENT;
SELECT * FROM STUDENT ORDER BY SEC;
SELECT * FROM STUDENT ORDER BY SEC DESC;
SELECT * FROM STUDENT ORDER BY SEC,F_NAME;
SELECT ROLL,CONCAT(CONCAT(CONCAT(F_NAME,' '),CONCAT(M_NAME,' ')),L_NAME) AS "NAME", LPAD(CITY,5,'*S') FROM STUDENT; 
SELECT CITY FROM STUDENT; 
SELECT * FROM STUDENT WHERE DIV IS NULL;
SELECT ROLL,F_NAME,DECODE(DIV,1,'FIRST',2,'SECOND',3,'THIRD',0,'FAIL',NULL,'NOT AWARDED') FROM STUDENT;
SELECT ROLL,F_NAME,DECODE(SEC,'A',1,'B',2) AS SECTION FROM STUDENT;
