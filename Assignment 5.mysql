create table student (ROLL NUMBER(2),F_NAME VARCHAR2(23),M_NAME VARCHAR2(22),L_NAME VARCHAR2(20),SEC VARCHAR2(21),CITY VARCHAR2(20),AREA VARCHAR2(29),HOUSENO VARCHAR2(13),DIV NUMBER(2));
